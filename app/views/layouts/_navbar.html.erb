<% if user_signed_in? %>
  <%= button_to "logout", destroy_user_session_path, method: :delete %>
  <%= link_to "browse", browse_path %>
  <%= link_to "edit account", edit_user_registration_path %>
  <br>
  <%= link_to "Create new Raffle", new_raffle_path %>
  <br>
  
  <%= link_to "My raffles", user_raffles_path(current_user) %>
  <%= link_to "Entered raffles", raffle_entries_path(current_user) %>
  <%= render "raffles/modal" %><%= number_to_currency(current_user.funds) %>
  <br>
<% else %>
  <%= link_to "login", new_user_session_path %> or <%= link_to "sign up", new_user_registration_path %> 
<% end %>